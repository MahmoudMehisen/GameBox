<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mina.gamebox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SolitaireGameActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SolitaireDifficultyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SolitaireStartpage"
            android:screenOrientation="portrait" />
        <activity android:name=".Klondike" />
        <activity android:name=".FlappyBirdGame">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".startFlappyBirdGame" />
        <activity android:name=".flappyBirdGameView"></activity>
    </application>

</manifest>